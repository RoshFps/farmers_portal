<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    tools:context=".ContactUsActivity">

    <TextView
        android:id="@+id/textView67"
        android:layout_width="0dp"
        android:layout_height="50sp"
        android:background="#087E7A"
        android:gravity="center"
        android:text="@string/ContactUs"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView68"

        android:layout_width="127dp"
        android:layout_height="30dp"
        android:layout_marginBottom="412dp"
        android:background="#F0FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:gravity="bottom|clip_horizontal|center|clip_vertical|right|fill"
        android:includeFontPadding="true"

        android:text="9353******"
        android:textColor="#151515"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20sp"
        android:layout_marginTop="40sp"
        android:text="@string/or"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toEndOf="@+id/textView68"
        app:layout_constraintTop_toBottomOf="@+id/textView67"
        app:layout_constraintVertical_bias="0.328" />

    <TextView
        android:id="@+id/textView71"
        android:layout_width="128dp"
        android:layout_height="28dp"
        android:layout_marginBottom="412dp"
        android:background="#F0FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:gravity="bottom|clip_horizontal|center|clip_vertical|right|fill"
        android:includeFontPadding="true"

        android:text="7256******"
        android:textColor="#151515"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"

        />

    <TextView
        android:id="@+id/textView72"
        android:layout_width="382dp"
        android:layout_height="165dp"
        android:layout_marginBottom="208dp"
        android:background="#F0FFFFFF"
        android:fontFamily="sans-serif-medium"
        android:gravity="bottom|clip_horizontal|center|clip_vertical|right|fill"
        android:includeFontPadding="true"
        android:text="If you have any query please contact us on the above numbers or If you want to add any product please SMS or contact us on the above number."
        android:textColor="#151515"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>